{
  "name": "back-end",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "pretest:e2e": "dotenv -c test -- docker compose down -v && dotenv -c test -- docker compose up -d && sleep 2 && dotenv -c test -- npx prisma migrate reset --force",
    "test:e2e": "dotenv -c test -- jest --runInBand --no-cache --watch --silent=false --config test/jest-e2e.json",
    "db:restart": "docker compose down -v && docker compose up -d && sleep 2"
  },
  "dependencies": {
    "@nestjs/axios": "^3.0.1",
    "@nestjs/common": "^10.3.0",
    "@nestjs/core": "^10.3.0",
    "@nestjs/platform-express": "^10.3.0",
    "axios": "^1.6.2",
    "nodemailer": "^6.9.7",
    "reflect-metadata": "^0.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "@nestjs/cli": "^10.2.1",
    "@nestjs/config": "^3.1.1",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-socket.io": "^10.3.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.3.0",
    "@nestjs/websockets": "^10.3.0",
    "@prisma/client": "^5.7.1",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "@types/nodemailer": "^6.4.14",
    "@types/passport": "^1.0.13",
    "@types/passport-42": "^1.1.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "argon2": "^0.31.1",
    "babel-jest": "^29.7.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.0",
    "express-session": "^1.17.3",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "jest": "^29.7.0",
    "jsonwebtoken": "^9.0.2",
    "passport": "^0.7.0",
    "passport-42": "^1.2.6",
    "passport-jwt": "^4.0.1",
    "prettier": "^3.1.1",
    "prisma": "^5.7.1",
    "rxjs": "^7.8.1",
    "socket.io": "^4.7.2",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
